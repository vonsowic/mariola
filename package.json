{
  "name": "mariola",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "postinstall": "cd client && npm install",
    "client": "cd client && npm start",
    "server": "nodemon app.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "concurrently": "^3.5.1",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.15.5",
    "jsonwebtoken": "^8.2.0",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.1",
    "sequelize": "^4.35.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "nodemon": "^1.17.2"
  }
}
